<hibernate-mapping package="fms.model.maintenance">
	<class name="Cost" table="cost">
		<id name="requestID" type="int" column="requestID" />
        <property name="maintenanceCost" type="int" column="maintenanceCost"/>
	</class>
</hibernate-mapping>

