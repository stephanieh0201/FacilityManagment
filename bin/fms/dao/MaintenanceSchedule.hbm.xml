<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="fms.model.maintenance">
	<class name="MaintenanceSchedule" table="maintenanceSchedule">
		<id name="facilityID" type="int" column="facilityID" />
        <property name="reserveDate" type="date" column="reserveDate"/>
        <property name="status" type="boolean" column="status"/>
	</class>
</hibernate-mapping>

