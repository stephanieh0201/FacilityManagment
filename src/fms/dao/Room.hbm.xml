<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="fms.model.facility">
	<class name="Room" table="room">
		<id name="roomID" column="roomId" type="int"/>
		<property name="facilityID" column= "facilityId" type="int"></property>
		<property name="capacity" type="int" column="capacity"/>
	</class>
</hibernate-mapping>